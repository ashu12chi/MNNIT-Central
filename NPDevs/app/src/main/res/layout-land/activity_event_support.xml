<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EventSupport">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="0dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="vertical"
            android:paddingBottom="192dp">

            <ImageView
                android:id="@+id/imageView14"
                android:layout_width="225dp"
                android:layout_height="225dp"
                android:padding="8dp"
                android:paddingBottom="4dp"
                app:srcCompat="@drawable/avishkar" />

            <ImageView
                android:id="@+id/imageView13"
                android:layout_width="225dp"
                android:layout_height="225dp"
                android:padding="4dp"
                app:srcCompat="@drawable/culrav" />

            <ImageView
                android:id="@+id/imageView15"
                android:layout_width="225dp"
                android:layout_height="225dp"
                android:padding="4dp"
                app:srcCompat="@drawable/eloquence" />

            <ImageView
                android:id="@+id/imageView16"
                android:layout_width="225dp"
                android:layout_height="225dp"
                android:padding="4dp"
                app:srcCompat="@drawable/prosang" />

            <ImageView
                android:id="@+id/imageView17"
                android:layout_width="225dp"
                android:layout_height="225dp"
                android:padding="4dp"
                app:srcCompat="@drawable/renaissance" />

            <ImageView
                android:id="@+id/imageView18"
                android:layout_width="225dp"
                android:layout_height="225dp"
                android:padding="4dp"
                app:srcCompat="@drawable/annual" />

            <ImageView
                android:id="@+id/imageView19"
                android:layout_width="225dp"
                android:layout_height="225dp"
                android:padding="4dp"
                app:srcCompat="@drawable/clubs" />

        </LinearLayout>
    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>