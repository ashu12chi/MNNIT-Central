<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Powersurge">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView153"
                android:layout_width="225dp"
                android:layout_height="225dp"
                android:padding="4dp"
                app:srcCompat="@drawable/codespark" />

            <ImageView
                android:id="@+id/imageView154"
                android:layout_width="225dp"
                android:layout_height="225dp"
                android:padding="4dp"
                app:srcCompat="@drawable/pentathlon" />

            <ImageView
                android:id="@+id/imageView155"
                android:layout_width="225dp"
                android:layout_height="225dp"
                android:padding="4dp"
                app:srcCompat="@drawable/predefinedhardware" />

            <ImageView
                android:id="@+id/imageView156"
                android:layout_width="225dp"
                android:layout_height="225dp"
                android:padding="4dp"
                app:srcCompat="@drawable/warofcurrents" />
        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>